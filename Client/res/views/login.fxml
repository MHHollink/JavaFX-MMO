<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>

<Pane prefHeight="400.0" minHeight="400.0" maxHeight="400.0" prefWidth="600.0" minWidth="600.0" maxWidth="600.0"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="nl.marcusink.mmo.client.controller.LoginController"
      styleClass="root" >
   <children>

        <TextField fx:id="username" layoutX="408.0" layoutY="298.0" prefHeight="25.0" prefWidth="172.0" promptText="Username" />

        <PasswordField fx:id="password" layoutX="408.0" layoutY="331.0" prefHeight="25.0" prefWidth="172.0" promptText="Password" />

        <Button fx:id="submitLogin" onAction="#handleLoginSubmit" layoutX="408.0" layoutY="365.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="66.0" text="Login" textAlignment="CENTER" />

        <Button fx:id="register" onAction="#handleRegister" layoutX="514.0" layoutY="365.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="66.0" text="Register" textAlignment="CENTER" />

   </children>

    <stylesheets>
        <URL value="@../styles/stylingTemplate.css" />
        <URL value="@../styles/login.css" />
    </stylesheets>
</Pane>
